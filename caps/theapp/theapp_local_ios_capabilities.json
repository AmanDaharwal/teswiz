{
  "iOS": {
    "app": "https://github.com/anandbagmar/sampleAppsForNativeMobileAutomation/raw/main/TheApp.zip",
    "appium:iPhoneOnly": true,
    "automationName": "XCUITest",
    "clearSystemFiles": true,
    "deviceName": "iPhone 15 Pro",
    "maxTypingFrequency": 10,
    "noReset": false,
    "platformName": "iOS",
    "platformVersion": "17.4",
    "preventWDAAttachments": true,
    "printPageSourceOnFindFailure": true,
    "shouldUseSingletonTestManager": false,
    "simpleIsVisibleCheck": true,
    "usePrebuiltWDA": false
  },
  "serverConfig" : {
    "server": {
      "use-plugins": ["device-farm", "relaxed-caps"],
      "plugin": {
        "device-farm": {
          "platform": "iOS",
          "skipChromeDownload": true,
          "iosDeviceType": "simulated"
        }
      }
    }
  },
  "appiumServerPath": "./node_modules/appium/build/lib/main.js"
}
